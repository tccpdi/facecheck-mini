<Window x:Class="Facecheck_mini.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Facecheck_mini"
        mc:Ignorable="d"
        Title="MainWindow" Height="330.116" Width="433.397">
    <StackPanel>
        <Grid Width="320" Height="180" x:Name="gridmain" HorizontalAlignment="Center" VerticalAlignment="Top">
            <Image HorizontalAlignment="Stretch" x:Name="imgReg" Visibility="Hidden"  VerticalAlignment="Stretch"  Stretch="Fill" Width="320" Height="180" />
            <Image HorizontalAlignment="Stretch" x:Name="frameHolder"  VerticalAlignment="Stretch"  Stretch="Fill" Width="320" Height="180" />
        </Grid>
        <Grid >
            <Button x:Name="btnRegistrar" Content="Registrar"  VerticalAlignment="Top" HorizontalAlignment="Center" Width="252" Click="BtnRegistrar_Click"/>
            <TextBlock x:Name="txtRes" Text="" Margin="12,30,15.2,0" Height="70" Width="400" VerticalAlignment="Top" />
        </Grid>
    </StackPanel>
</Window>
